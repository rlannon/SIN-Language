alloc int myInt;
alloc ptr[int] myIntPtr;
alloc ptr[ptr] myPtrPtr;

let myIntPtr = $myInt;
let myPtrPtr = $myIntPtr;

let *myIntPtr = 10;
@print(*myIntPtr);

let **myPtrPtr = 20;
@print(**myPtrPtr);

alloc ptr[ptr] someOtherPtr;
let someOtherPtr = $myPtrPtr;
let ***someOtherPtr = 1000;

@print(***someOtherPtr);
